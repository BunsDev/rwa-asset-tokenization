{
  "name": "rwa-tokenization-contracts",
  "version": "1.0.0",
  "description": "RWA tokenization using Chainlink Function on Avalanche.",
  "main": "index.js",
  "repository": "https://github.com/BunsDev/rwa-asset-tokenization.git",
  "author": "Buns Enchantress <bunsthedev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "forge test -vv",
    "prices": "node script/getPrices.js",
    "metadata": "node script/getMetadata.js",
    "deploy": "forge script script/RealEstate.s.sol:RealEstateScript --rpc-url https://rpc.ankr.com/avalanche_fuji --broadcast --verifier-url https://api.routescan.io/v2/network/testnet/evm/43113/etherscan --etherscan-api-key verifyContract",
    "signin": "npx env-enc set-pw",
    "setup": "npx env-enc set",
    "flat": "forge flatten --output src/flats/RealEstate.sol src/RealEstate.sol",

    "subscribe": "npx hardhat func-sub-create --network avalancheFuji --amount 2 --contract 0x8e70fD24309B027a5309B6A6b182836b51000AcD",
    "issue": "npx hardhat func-issue --network avalancheFuji --contract 0x8e70fD24309B027a5309B6A6b182836b51000AcD --to 0x6D3cF2d1D7e113a2687dF9080CC490e176F53760 --subid 9614",
    "update": "npx hardhat func-update --network avalancheFuji --contract 0x8e70fD24309B027a5309B6A6b182836b51000AcD --tokenid 0 --subid 9614",
    "request": "npx hardhat func-request --network avalancheFuji --contract 0x8e70fD24309B027a5309B6A6b182836b51000AcD --subid 9614",
    "read": "npx hardhat func-read --network avalancheFuji --contract 0x8e70fD24309B027a5309B6A6b182836b51000AcD",

    "assign": "npx hardhat func-sub-add --network avalancheFuji --contract 0x8e70fD24309B027a5309B6A6b182836b51000AcD --subid 9614"
    },
    "dependencies": {
      "@chainlink/env-enc": "^1.0.5",
      "@chainlink/functions-toolkit": "^0.2.8"
    },
    "devDependencies": {
      "@nomicfoundation/hardhat-chai-matchers": "^1.0.3",
      "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
      "@nomicfoundation/hardhat-toolbox": "^2.0.0",
      "@nomicfoundation/hardhat-ethers": "^3.0.5",
      "@nomicfoundation/hardhat-foundry": "^1.1.1",
      "@nomiclabs/hardhat-ethers": "^2.2.2",
      "@nomiclabs/hardhat-etherscan": "^3.1.0",
      "@typechain/ethers-v5": "^10.1.0",
      "@typechain/hardhat": "^9.1.0",
      "chai": "^4.3.6",
      "hardhat": "^2.22.2",
      "hardhat-contract-sizer": "^2.6.1",
      "hardhat-gas-reporter": "^2.1.0",
      "ora": "5.4.1",
      "solidity-coverage": "^0.8.2",
      "typechain": "^8.3.2"
    }
}
